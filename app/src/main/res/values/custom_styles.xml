<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Circular">
        <item name="cornerSize">50%</item>
    </style>

    <style name="BottomNavigationViewStyle" parent="Widget.Design.BottomNavigationView">
        <item name="labelVisibilityMode">labeled</item>
    </style>

    <style name="RoundedSquare">
        <item name="cornerSize">8%</item>
        <item name="backgroundColor">@color/my_light_primary</item>
<!--        <item name="android:background">@drawable/rounded_square_background</item>-->
    </style>

    <style name="CornerCut">
        <item name="cornerSize">15dp</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="DiamondCut">
        <item name="cornerSize">75dp</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="SpecificCornerCut">
        <item name="cornerSizeTopRight">75dp</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerSizeBottomLeft">75dp</item>
        <item name="cornerFamilyBottomLeft">cut</item>
    </style>

    <style name="SpecificCornerRounded">
        <item name="cornerSizeTopRight">75dp</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeBottomLeft">75dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
    </style>

</resources>